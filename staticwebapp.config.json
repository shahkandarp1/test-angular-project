{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/api",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/styles/*", "/scripts/*"]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
